{
  "name": "everscale-random",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "description": "Everscale random",
  "scripts": {
    "nvm": "echo $npm_package_engines_node | cut -d'=' -f2",
    "build-data": "wget --output-document=asset/sample.json https://gist.githubusercontent.com/ilyar/3ddd97cc0cef1f1c57ac690d8ae2522d/raw/5ffc8ca339ee7deade20c056b2a509fbbc144d2e/data-for-ama.json",
    "build-asset": "convert-svg-to-png --width 48 asset/icon.svg",
    "build-web": "yarn build-deps && mkdir -p dist && parcel build --public-url ${URL:-/} --no-source-maps --out-dir dist --out-file index.html src/App.html",
    "build-deps": "yarn build-asset && yarn build-data",
    "clear": "rm -fr dist",
    "build": "yarn clear && yarn build-web",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "yarn build && gh-pages --repo $(git remote --verbose | grep push | cut -f2 | cut -d' ' -f1) --dist dist",
    "prestart": "yarn build-deps",
    "start": "parcel src/App.html"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {},
  "dependencies": {
    "bootstrap": "^5.1.3",
    "broxus-ton-tokens-contracts": "broxus/ton-eth-bridge-token-contracts",
    "convert-svg-to-png": "^0.5.0",
    "everdev": "^1.1.1",
    "everscale-inpage-provider": "^0.3.9",
    "everscale-standalone-client": "^1.0.3",
    "gh-pages": "^3.2.3",
    "parcel": "^2.4.1",
    "parcel-bundler": "^1.12.5",
    "serve": "^13.0.2",
    "typescript": "^4.6.3"
  }
}
